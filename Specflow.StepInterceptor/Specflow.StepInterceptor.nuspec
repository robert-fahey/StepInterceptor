        <file src=".\bin\Release\Specflow.StepInterceptor.dll.config" target="lib\net461\Specflow.StepInterceptor.dll.config" />
        <file src=".\bin\Release\Specflow.StepInterceptor.pdb" target="lib\net461\Specflow.StepInterceptor.pdb" />
    </files>
</package>