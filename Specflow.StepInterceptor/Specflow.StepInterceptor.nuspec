<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Specflow.StepInterceptor</id>
        <version>$version$</version>
        <title>Specflow.StepInterceptor</title>
        <authors>Nick Hammond,Rob Fahey</authors>
        <owners>Rob Fahey</owners>
        <licenseUrl>https://github.com/robert-fahey/StepInterceptor/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/robert-fahey/StepInterceptor</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Allows a collection of Actions to be invoked before or after a step type seamlessly. E.g. Invoke .build() on a builder when the first 'When' step is called. Removing the need for a filler step to build the object.</description>
        <tags>Specflow</tags>
        <dependencies>
            <dependency id="SpecFlow" version="2.1.0" />
        </dependencies>
    </metadata>
